{
  "name": "schwab-rebalancer",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "author": "Daniel Yeoman",
  "version": "0.1.0-alpha.0",
  "description": "A comprehensive portfolio management platform built for sophisticated tax-loss harvesting and asset allocation with Charles Schwab.",
  "homepage": "https://github.com/sudowealth/schwab-rebalancer",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudowealth/schwab-rebalancer.git"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "portfolio",
    "rebalancing",
    "tax-loss",
    "harvesting",
    "schwab",
    "rebalancer"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "fix": "biome check --write .",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:push:prod": "drizzle-kit push --config drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "seed": "tsx src/lib/seeds/main.ts",
    "generate-secrets": "tsx scripts/generate-secrets.ts",
    "precommit": "biome check --write"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@netlify/neon": "^0.1.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sudowealth/schwab-api": "^2.1.0",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-router": "^1.131.44",
    "@tanstack/react-router-devtools": "^1.131.44",
    "@tanstack/react-start": "^1.131.44",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "better-auth": "^1.3.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "exceljs": "^4.4.0",
    "knip": "^5.62.0",
    "lucide-react": "^0.544.0",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.2",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "yahoo-finance2": "^2.13.3",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
