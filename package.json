    "db:push:prod": "drizzle-kit push --config drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "seed": "tsx src/db/seeds/main.ts",
    "setup": "tsx scripts/setup.ts",
    "precommit": "biome check --write"
