{
  "name": "schwab-rebalancer",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "author": "Daniel Yeoman",
  "version": "0.1.0-alpha.0",
  "description": "A comprehensive portfolio management platform built for sophisticated tax-loss harvesting and asset allocation with Charles Schwab.",
  "homepage": "https://github.com/sudowealth/schwab-rebalancer",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudowealth/schwab-rebalancer.git"
  },
  "keywords": [
    "portfolio",
    "rebalancing",
    "tax-loss",
    "harvesting",
    "schwab",
    "rebalancer"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "deploy": "npm run build && wrangler deploy",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "cf-typegen": "wrangler types --env-interface Env",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply tax-loss-harvesting --local",
    "db:migrate:prod": "wrangler d1 migrations apply tax-loss-harvesting",
    "db:studio": "drizzle-kit studio",
    "seed": "tsx src/lib/seeds/main.ts",
    "generate-key": "tsx scripts/generate-encryption-key.ts"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250819.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sudowealth/schwab-api": "^2.1.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-router": "^1.131.27",
    "@tanstack/react-router-devtools": "^1.131.27",
    "@tanstack/react-start": "^1.131.27",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/better-sqlite3": "^7.6.13",
    "better-auth": "^1.3.7",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "exceljs": "^4.4.0",
    "knip": "^5.62.0",
    "lucide-react": "^0.540.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.2",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "yahoo-finance2": "^2.13.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.1",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
