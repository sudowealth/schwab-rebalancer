{
  "name": "schwab-rebalancer",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "author": "Daniel Yeoman",
  "version": "1.0.0",
  "description": "A comprehensive portfolio management platform built for sophisticated tax-loss harvesting and asset allocation with Charles Schwab.",
  "homepage": "https://github.com/sudowealth/schwab-rebalancer",
  "repository": {
    "type": "git",
